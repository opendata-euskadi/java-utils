<test>
	<!-- 
	Signature service used to sign the guaranteed files 
		A RFC is mandatory to get access to certificate with id=0035 
	-->
	<signatureService mock='true'>
		<wsURL>http://svc.intra.integracion.jakina.ejiedes.net/ctxapp/X43FNSHF2?WSDL</wsURL>
		<certificateId>0035</certificateId>
		<retryingAttempts>5</retryingAttempts>
		<stopAfterSeconds>120</stopAfterSeconds>
	</signatureService>

	<!-- 
	PIF is used by the signature service to sign the guaranteed files since the size of the ZIP file that
	contains all guaranteed files can be huge 
		AA88B MUST have XLNets app login in order to READ it's PIF zone (AA88B can WRITE at any other app PIF zone) 
	-->
	<pifService>
		<uiConsoleUrl>http://svc.integracion.jakina.ejiedes.net/y31dBoxWAR/appbox</uiConsoleUrl>
	</pifService>
	
	<!--
	DOKUSI 
	-->
	<dokusiService>
		<webServiceUrl>http://svc.extra.integracion.jakina.ejiedes.net:80/ctxapp/t65bFsd</webServiceUrl>
		<auditId>X42T#X42T</auditId>
		<requestedPifPath>/x42t/dokusi/</requestedPifPath>	<!-- the path where the DOKUSI web service is asked to put the file -->
 	</dokusiService>>
	
	<!-- XLNets -->
	<xlnets loginAppCode='x42t' token='httpProvided' login='app'>	
		<!--
		Token types:
			n38api			: use the http request cookies and N38 API
			mockFile 		: use a classpath-stored file with the xlnets session / user / auth / etc data 
										> the [sessionToken] element contains the session token's path (use https://xlnets.servicios.jakina.ejiedes.net/xlnets/servicios.htm to generate a token)
										> the [userDataToken] element contains the user info token's path (use https://xlnets.servicios.jakina.ejiedes.net/xlnets/servicios.htm to generate a token)
										> the [authToken] element contains the auth token's path (use https://xlnets.servicios.jakina.ejiedes.net/xlnets/servicios.htm to generate a token)
			httpProvided	: Using a service that provides xlnets session tokens
										> the [loginAppCode] attribute is mandatory
										> the [sessionToken] element contains the url of the service that provides tokens (ie: http://svc.intra.integracion.jakina.ejiedes.net/ctxapp/Y31JanoServiceXlnetsTokenCreatorServlet?login_app=appId)
		-->
		<!--
		Login types:
			user			: user login
			app				: app login
		-->
		<sessionToken>http://svc.intra.integracion.jakina.ejiedes.net/ctxapp/Y31JanoServiceXlnetsTokenCreatorServlet</sessionToken>
	</xlnets>
</test>